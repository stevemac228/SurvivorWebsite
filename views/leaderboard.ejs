<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/style.css">
    <title>Outdraft</title>
</head>
<body>
    <div class="nav">
        <a href="/" class="logo" id="home">Outdraft</a>
        <!--<a href="/newTeam" class="subhead" id="addTeam">Add Team</a>
            <a href="/stats" class="subhead" id="stats">Stats</a>
            <a href="/admin" class="subhead" id="admin">Admin</a>-->
    </div>
    <% for (var i in leaderboard) {%>
        <% console.log(leaderboard[i].value.Score)%>
        <p id="Name"><%= leaderboard[i].key %>: <%= leaderboard[i].value.Score%></p>
        <div class="teamContainer">
            <div class="image">
                <% if(leaderboard[i].value.P1.Out == true){%>
                    <img class="photoOut"src="/Images/<%= leaderboard[i].value.P1.Name %>.png">
                    <!--<div class="bottom-right-Out"><%=leaderboard[i].value.P1.Place%></div>-->
                <%}else{%>
                    <img class="photo"src="/Images/<%= leaderboard[i].value.P1.Name %>.png">
                    <!--<div class="bottom-right"><%=leaderboard[i].value.P1.Place%></div>-->
                <%}%>
            </div>
            <div class="image">
                <% if(leaderboard[i].value.P2.Out == true){%>
                    <img class="photoOut"src="/Images/<%= leaderboard[i].value.P2.Name %>.png">
                    <!--<div class="bottom-right-Out"><%=leaderboard[i].value.P2.Place%></div>-->
                <%}else{%>
                    <img class="photo"src="/Images/<%= leaderboard[i].value.P2.Name %>.png">
                    <!--<div class="bottom-right"><%=leaderboard[i].value.P2.Place%></div>-->
                <%}%>
            </div>
            <div class="image">
                <% if(leaderboard[i].value.P3.Out == true){%>
                    <img class="photoOut"src="/Images/<%= leaderboard[i].value.P3.Name %>.png">
                    <!--<div class="bottom-right-Out"><%=leaderboard[i].value.P3.Place%></div>-->
                <%}else{%>
                    <img class="photo"src="/Images/<%= leaderboard[i].value.P3.Name %>.png">
                    <!--<div class="bottom-right"><%=leaderboard[i].value.P3.Place%></div>-->
                <%}%>
            </div>
            <div class="image">
                <% if(leaderboard[i].value.P4.Out == true){%>
                    <img class="photoOut"src="/Images/<%= leaderboard[i].value.P4.Name %>.png">
                    <!--<div class="bottom-right-Out"><%=leaderboard[i].value.P4.Place%></div>-->
                <%}else{%>
                    <img class="photo"src="/Images/<%= leaderboard[i].value.P4.Name %>.png">
                    <!--<div class="bottom-right"><%=leaderboard[i].value.P4.Place%></div>-->
                <%}%>
            </div>
        </div>
      <% } %>
</body>
</html>
